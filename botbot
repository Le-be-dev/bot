import time

a = []

a.append('#')
a.append('#')
a.append('#')

k = 0

while k<=200:
    for i in a:
        print(i, end='')
    a[k]= ' '
    a.append('#')
    print('\n\n\n\n\n\n')
    time.sleep(0.0001)
    k=k+1

while k>0:
    for i in a:
        print(i, end='')
    a.pop()
    print('\n')
    time.sleep(0.5)
    k = k-1


